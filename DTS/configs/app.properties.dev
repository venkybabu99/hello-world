check_file_processed = SET NOCOUNT ON; DECLARE @filename VARCHAR(250) = '{0}', @processid INT = {1}, @filetypeid INT = {2}, @statusid INT = {3}; SELECT [FileId] FROM [dbo].[Files] z WHERE filename = @filename AND ProcessId = @processid AND EXISTS (SELECT (1) FROM [dbo].[Files] t WHERE z.FileId = t.ParentFileId AND t.FileTypeId = @filetypeid AND t.StatusId = @statusid)
check_file_processed_txt = SET NOCOUNT ON; DECLARE @filename VARCHAR(250) = '{0}', @processid INT = {1}, @filetypeid INT = {2}, @statusid INT = {3}; SELECT [FileId] FROM [dbo].[Files] z WHERE filename = @filename AND ProcessId = @processid AND FileTypeId = @filetypeid AND StatusId = @statusid
get_corp_tags = SET NOCOUNT ON; SELECT [Field] FROM [CorporationTags] AS [c] OUTER APPLY( SELECT * FROM [util].[dbo].[ParseDelimited] ([c].[Tags], ' ') AS [b] ) AS [b]
get_names_to_parse = SET NOCOUNT ON; SELECT [RecordId], [RawName1], [RawName2], [RawName3], [RawName4] FROM [dbo].[NameView] WHERE NULLIF(RawName1,'') IS NOT NULL OR NULLIF(RawName2,'') IS NOT NULL OR NULLIF(RawName3,'') IS NOT NULL OR NULLIF(RawName4,'') IS NOT NULL
update_parsed_names = SET NOCOUNT ON; UPDATE n SET Name1 = t.Name1, Name2 = t.Name2, Name3 = t.Name3, Name4 = t.Name4 FROM [dbo].[ParsedNames] n INNER JOIN [dbo].[TEMP_NAME_PARSED] t ON t.RecordId = n.TargetId
